<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns:fx="http://javafx.com/fxml" fx:controller="controller.Amministratore.VisualizzaOperazioniController">
    <top>
        <HBox fx:id="navbarContainer" />
    </top>

    <center>
        <TableView fx:id="tabellaOperazioni" layoutX="10" layoutY="10" prefWidth="780" prefHeight="400">
            <columns>
                <TableColumn fx:id="taxCodeDiabetologo" text="Diabetologo" />
                <TableColumn fx:id="operazione" text="operazione" />
                <TableColumn fx:id="taxCodePaziente" text="Paziente" />
                <TableColumn fx:id="dataOperazione" text="Data operazione" />
            </columns>
        </TableView>
    </center>
</BorderPane>

